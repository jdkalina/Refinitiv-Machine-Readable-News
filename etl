import pandas as pd
import tickFiles as trth
import mrnProcessing as mrn

outputFiles = []
fls = mrn.findFiles(fileDir = /home/josh/Documents/mrn/')
db = mrn.to_dataframe(fls)
db = mrn.filter_output(db)
tk = trth.generateToken()
for i,v in db.iterrows():
    rU,rH,rB = trth.generateRequestBody(token=tk,dataframe=db,index=0)
    f = trth.generateFiles(token = tk, requestUrl=rU,requestHeaders=rH,requestBody=rB, iterator = i)
    outputFiles.append(f)
